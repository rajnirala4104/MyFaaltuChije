{
   "manifest_version": 3,
   "name": "Demo Extension",
   "description": "This Extension is for learning purpose.",
   "version": "1.0.0",
   "action": {
      "default_popup": "./popup/popup.html"
   },
   "background": {
      "service_worker": "./background.js"
   },
   "permissions": ["activeTab", "tabs", "scripting"],
   "host_permissions": ["<all_urls>"]
}
