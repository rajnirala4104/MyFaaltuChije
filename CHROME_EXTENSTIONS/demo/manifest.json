{
   "manifest_version": 3,
   "name": "Demo Extension",
   "description": "Demo Extension",
   "version": "1.0.0",
   "action": {
      "default_popup": "./popup/popup.html"
   },
   "content_scripts": [
      {
         "js": ["./scripts/content.js"],
         "matches": ["https://github.com/*"]
      }
   ],
   "background": {
      "service_worker": "./background.js"
   },
   "permissions": ["activeTab"]
}
