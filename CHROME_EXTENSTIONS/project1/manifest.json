{
   "manifest_version": 3,
   "name": "Project",
   "description": "A project for testing ang more for learning..",
   "version": "1.0.0",
   "action": {
      "default_popup": "./popup/popup.html"
   },
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "js": ["./scripts/content.js"]
      }
   ],
   "permissions": ["activeTab", "scripting", "storage", "tabs"],
   "host_permissions": ["<all_urls>"]
}
